<div class="ibox float-e-margins">
  <div class="ibox-title">
    <h5>Category form </h5>
  </div>
  <div class="ibox-content">
    <form class="edit-category-form form-horizontal">

      <div class="form-group"><label class="col-sm-2 control-label">Title</label>
        {% include '@KateshchFaq/Widgets/Admin/translateView.twig' with {'langs': langs,'fieldName': 'title'} %}
      </div>
      <div class="form-group"><label class="col-sm-2 control-label">Class</label>
        <div class="col-sm-10">
          <input name="class" placeholder="Type your message here">
        </div>

        <div class="form-group">
          <div class="col-sm-4 col-sm-offset-2">
            <button class="btn btn-white" type="submit">Cancel</button>
            <button type="submit" class="btn btn-primary save-question">Save</button>
          </div>
        </div>

    </form>
  </div>
</div>